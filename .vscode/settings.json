{
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "eslint.useFlatConfig": true,
  "eslint.options": {
    "overrideConfigFile": "./eslint.config.js"
  },
  "prettier.configPath": "./prettier.config.cjs",
  "stylelint.validate": ["stylus"],
  "cSpell.words": [
    "animitter",
    "autobuffer",
    "bytediff",
    "camelcase",
    "chpt",
    "classlist",
    "crossorigin",
    "deepmerge",
    "derequire",
    "getusermedia",
    "ICPU",
    "initialise",
    "isom",
    "isomiso",
    "KHTML",
    "Lavf",
    "Mbit",
    "mofo",
    "MSIE",
    "nanoevents",
    "nvmrc",
    "Ooops",
    "packagejson",
    "Pivac",
    "playsinline",
    "plusplus",
    "preend",
    "prefinish",
    "rsbuild",
    "RSDOCTOR",
    "rslib",
    "rspack",
    "rubberband",
    "Sadler",
    "Seeflow's",
    "styl",
    "Tarr",
    "Teleporting",
    "tseslint",
    "videomail",
    "videomails",
    "Vitest",
    "websockets",
    "whatisstored",
    "wohooo",
    "xvfb",
    "Zack"
  ]
}
