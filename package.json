{
  "name": "videomail-client",
  "version": "10.3.4",
  "description": "A wicked npm package to record videos directly in the browser, wohooo!",
  "keywords": [
    "webcam",
    "video",
    "videomail",
    "encoder",
    "getusermedia",
    "audio",
    "recorder"
  ],
  "homepage": "https://videomail.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binarykitchen/videomail-client.git"
  },
  "license": "CC0-1.0",
  "author": "Michael Heuberger <michael.heuberger@binarykitchen.com>",
  "contributors": [
    {
      "name": "Michael Heuberger",
      "email": "michael.heuberger@binarykitchen.com"
    }
  ],
  "type": "module",
  "exports": {
    ".": {
      "umd": "./dist/umd/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js"
    }
  },
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "audit": "npx audit-ci --config audit-ci.json",
    "build:prod": "cross-env NODE_ENV=production rslib build",
    "build:storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=chpt_742ead9ab882938",
    "lint": "eslint --color .",
    "lint:fix": "eslint --color --fix .",
    "lint:inspect": "eslint --inspect-config",
    "prettier": "prettier --check ./etc ./src ./.storybook ./*.ts",
    "prettier:fix": "prettier --write ./etc ./src ./.storybook ./*.ts",
    "release": "release-it --only-version --config ./etc/release-it.ts",
    "storybook": "cross-env BROWSER=chromium storybook dev -p 8443 --https --ssl-cert ./etc/ssl-certs/localhost.crt --ssl-key ./etc/ssl-certs/localhost.key",
    "test": "cross-env ENVIRON=test vitest --watch=false",
    "types": "tsc --noEmit",
    "types:config": "tsc --showConfig"
  },
  "prettier": "./prettier.config.cjs",
  "dependencies": {
    "animitter": "3.0.0",
    "audio-sample": "4.1.0",
    "canvas-to-buffer": "4.1.2",
    "contains": "0.1.1",
    "deepmerge": "4.3.1",
    "defined": "1.0.1",
    "document-visibility": "1.0.1",
    "get-form-data": "3.0.0",
    "hidden": "1.1.1",
    "is-power-of-two": "1.0.0",
    "nanoevents": "9.1.0",
    "serialize-error": "12.0.0",
    "superagent": "10.2.2",
    "ua-parser-js": "2.0.4",
    "websocket-stream": "5.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.0.1",
    "@eslint/config-inspector": "1.1.0",
    "@eslint/js": "9.31.0",
    "@rsbuild/plugin-node-polyfill": "1.3.1",
    "@rsbuild/plugin-stylus": "1.1.2",
    "@rsdoctor/rspack-plugin": "1.1.8",
    "@rslib/core": "0.10.6",
    "@storybook/addon-a11y": "9.0.17",
    "@storybook/addon-docs": "9.0.17",
    "@storybook/addon-links": "9.0.17",
    "@storybook/html": "9.0.17",
    "@tsconfig/node22": "22.0.2",
    "@tsconfig/strictest": "2.0.5",
    "@types/defined": "1.0.2",
    "@types/node": "22.15.30",
    "@types/superagent": "8.1.9",
    "@types/ua-parser-js": "0.7.39",
    "@vitest/eslint-plugin": "1.3.4",
    "audit-ci": "7.1.0",
    "chromatic": "13.1.2",
    "cross-env": "7.0.3",
    "eslint": "9.31.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-de-morgan": "1.3.0",
    "eslint-plugin-depend": "1.2.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-package-json": "0.45.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-regexp": "2.9.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-storybook": "9.0.17",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "msw": "2.10.4",
    "msw-storybook-addon": "2.0.5",
    "prettier": "3.6.2",
    "prettier-plugin-curly": "0.3.2",
    "prettier-plugin-packagejson": "2.5.18",
    "prettier-plugin-sh": "0.18.0",
    "release-it": "19.0.4",
    "storybook": "9.0.17",
