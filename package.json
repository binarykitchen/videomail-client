{
  "name": "videomail-client",
  "version": "10.0.0-3",
  "description": "A wicked npm package to record videos directly in the browser, wohooo!",
  "keywords": [
    "webcam",
    "video",
    "videomail",
    "encoder",
    "getusermedia",
    "audio",
    "recorder"
  ],
  "homepage": "https://videomail.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binarykitchen/videomail-client.git"
  },
  "license": "CC0-1.0",
  "author": "Michael Heuberger <michael.heuberger@binarykitchen.com>",
  "contributors": [
    {
      "name": "Michael Heuberger",
      "email": "michael.heuberger@binarykitchen.com"
    }
  ],
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "audit": "npx audit-ci --config audit-ci.json",
    "build:prod": "cross-env NODE_ENV=production rsbuild build -c ./etc/rsbuild/config.ts",
    "build:storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=chpt_742ead9ab882938",
    "lint": "eslint --color .",
    "lint:fix": "eslint --color --fix .",
    "prettier": "prettier --check ./etc ./src ./.storybook",
    "prettier:fix": "prettier --write ./etc ./src ./.storybook",
    "release": "release-it --only-version --config ./etc/release-it.ts",
    "storybook": "cross-env BROWSER=chromium storybook dev -p 8443 --https --ssl-cert ./etc/ssl-certs/localhost.crt --ssl-key ./etc/ssl-certs/localhost.key",
    "test": "cross-env ENVIRON=test vitest",
    "types:check": "tsc --noEmit",
    "types:config": "tsc --showConfig"
  },
  "prettier": "./prettier.config.cjs",
  "dependencies": {
    "animitter": "3.0.0",
    "audio-sample": "4.1.0",
    "canvas-to-buffer": "4.1.1",
    "contains": "0.1.1",
    "deepmerge": "4.3.1",
    "defined": "1.0.1",
    "document-visibility": "1.0.1",
    "filesize": "10.1.6",
    "get-form-data": "3.0.0",
    "hidden": "1.1.1",
    "humanize-duration": "3.32.1",
    "is-power-of-two": "1.0.0",
    "nanoevents": "9.1.0",
    "superagent": "10.1.1",
    "ua-parser-js": "1.0.39",
    "websocket-stream": "5.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.2",
    "@eslint/js": "9.13.0",
    "@rsbuild/core": "1.0.19",
    "@rsbuild/plugin-node-polyfill": "1.2.0",
    "@rsbuild/plugin-stylus": "1.0.5",
    "@rsdoctor/rspack-plugin": "0.4.7",
    "@storybook/addon-a11y": "8.4.0",
    "@storybook/addon-console": "3.0.0",
    "@storybook/addon-essentials": "8.4.0",
    "@storybook/addon-interactions": "8.4.0",
    "@storybook/addon-links": "8.4.0",
    "@storybook/html": "8.4.0",
    "@storybook/manager-api": "8.4.0",
    "@storybook/theming": "8.4.0",
    "@tsconfig/node22": "22.0.0",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.8.6",
    "@types/superagent": "8.1.9",
    "@types/ua-parser-js": "0.7.39",
    "@vitest/eslint-plugin": "1.1.7",
    "audit-ci": "7.1.0",
    "chromatic": "11.16.3",
    "cross-env": "7.0.3",
    "eslint": "9.13.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-depend": "0.11.0",
    "eslint-plugin-import-x": "4.4.0",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-regexp": "2.6.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-storybook": "0.10.1",
    "globals": "15.11.0",
    "jsdom": "25.0.1",
    "msw": "2.6.0",
    "msw-storybook-addon": "2.0.3",
    "prettier": "3.3.3",
    "prettier-plugin-curly": "0.3.1",
    "prettier-plugin-packagejson": "2.5.3",
    "prettier-plugin-sh": "0.14.0",
    "release-it": "17.10.0",
    "rsbuild-plugin-dts": "0.0.15",
    "storybook": "8.4.0",
    "storybook-html-rsbuild": "0.1.2",
    "typescript": "5.6.3",
    "typescript-eslint": "8.12.2",
